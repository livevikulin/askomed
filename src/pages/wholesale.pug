extends ../views/layouts/default
block variables
	- var title = "Главная оптовикам"
	- var main = false;
	- var wholesale = true;
	
block content
	main
		.wholesale
			._banner
				.container
					span Оптовые поставки иммунобиологических препаратов, экспресс-тестов и изделий медицинского назначения для юридических лиц
