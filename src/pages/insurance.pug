extends ../views/layouts/default
block variables
	- var title = "Страховым компаниям"
	- var main = false;
	- var wholesale = true;
	
block content
	main
		.breadcrumbs
			.container
				.breadcrumbs-item
					a(href="#") Главная
					span О компании
		.insurance
			.container
				.insurance-block
					h1 Страховым компаниям
					span._subtitle Сервисные программы профилактики клещевых инфекций по ДМС
					span._choice Выберите нужную:
					.insurance-boxes
						.insurance-box
							._image
								img(src="img/insurance1.jpg", alt="")
							a._link(href="#") www.imkart.ru
							ul.succsess-list
								li
									p Прямой доступ застрахованного к получению услуги;
								li
									p Основной документ для обращения – Иммунокарта;
								li
									p Фиксированная стоимость услуги для СК;
								li
									p Территория обслуживания – РФ (преимущественно СФО, УФО);
								li
									p Сервисные ресурсы: памятка застрахованному, горячая линия, сайт imkart.ru, мобильное приложение;
								li
									p Подходит для агентского и корпоративного канала продаж, продаж в офисах СК.
						.insurance-box
							._image
								img(src="img/insurance2.jpg", alt="")
							a._link(href="#") зеленыйпункт.рф
							ul.succsess-list
								li
									p Получение услуги только через обращение в контакт-центр (КЦ);
								li
									p Основание для получения помощи – гарантийное письмо в МО от КЦ;
								li
									p Перевыставление стоимости услуги соисполнителей;
								li
									p Территория обслуживания – РФ (эндемичные регионы);
								li
									p Поэтапное сопровождение застрахованного при получении помощи;
								li
									p Сервисные ресурсы: горячая линия, сайт зеленыйпункт.рф;
								li
									p Онлайн канал продаж, партнерские каналы, коробочные продукты.
		.partners.insurance-partners
			.container
				.partners-block
					span._title Партнеры
					.partners-boxes
						.partner
							._image
